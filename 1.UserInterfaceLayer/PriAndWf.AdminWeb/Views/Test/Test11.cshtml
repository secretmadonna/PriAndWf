@model PriAndWf.Infrastructure.Helper.CodeExecuteTimeHelper.CodeExecuteTime

@{
    ViewBag.Title = "测试：代码执行时间";
    var list = Model.ExecuteTimes.OrderBy(m => m.Key);
    var totalCount = Model.ExecuteTimes.Sum(m => m.Value);
}

@section PagePluginStyles{
}
@section PageInlineStyles{
}


<h5>代码执行时间</h5>

@foreach (var item in list)
{
    <p>@(string.Format("耗时（{0}/{1}） : {2} s", item.Value, totalCount, item.Key))</p>
}
<p>@(string.Format("平均耗时 : {0} s", Model.GetExFirstLastAvg))</p>
@for (int i = 0; i < Model.GcCollectionCounts.Count; i++)
{
    <p>@(string.Format("Gen {0} : {1}", i, Model.GcCollectionCounts[i]))</p>
}


@section PagePluginScripts{
}
@section PageInlineScripts{
}
